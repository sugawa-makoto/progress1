<div class="container">
    <div class="wrapper_outside">
        <div class="app_title">
            <h1>Work progress</h1>
        </div>
        <div class="corporation_name">
            <h2>馬場電気株式会社</h2>
        </div>
            <ul class="nav nav-tabs">
              <li role="presentation" class="active"><a href="/">Home</a></li>
              <li role="presentation"><a href="#">出勤</a></li>
              <li role="presentation"><a href="#">現場</a></li>
              <li role="presentation"><a href="#">移動</a></li>
              <li role="presentation"><a href="#">休憩</a></li>
              <li role="presentation"><a href="#">日報</a></li>
              <li role="presentation"><a href="#">退勤</a></li>
              <li role="presentation"><a href="#">予定</a></li>
            </ul>
        <div class="corporation_name">
            <ol class="breadcrumb">
              <li><a href="/">Home</a></li>
              <li><a href="admin">各種データベース一覧</a></li>
              <li class="active">社員データベース</li>
            </ol>
        </div>
        <div class="wrapper_move_1">
            <form class="form-inline">
                <%= form_tag("/create") do %>
                        <div class="form-group">
                            <%= label_tag "form-title", "地点１" %>
                            <%= text_field :memos, :title, id:"form-title", class:"form-control", placeholder: "入力してください"%>
                        </div>
                        <div class="form-group">
                            <%= label_tag "form-content", "地点２" %>
                            <%= text_field :memos, :body, id:"form-content", class:"form-control", placeholder: "入力してください"%>
                        </div>
                        <div class="radio">
                          <label>
                            <input type="radio" name="optionsRadios" id="optionsRadios1" value="option1" checked>
                            出発
                          </label>
                        </div>
                        <div class="radio">
                          <label>
                            <input type="radio" name="optionsRadios" id="optionsRadios1" value="option1" checked>
                            到着
                          </label>
                        </div>
                    <%= submit_tag "通知" %>
                <% end %> 
            </form>
        </div>
    </div>
</div>